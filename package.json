{
  "name": "phosphor",
  "version": "0.4.0",
  "description": "The PhosphorJS libary.",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "expect.js": "^0.3.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "typescript": "^1.8.0",
    "webpack": "^1.12.14"
  },
  "scripts": {
    "clean:src": "rimraf lib",
    "clean:test": "rimraf test/build && rimraf test/coverage",
    "clean": "npm run clean:src && npm run clean:test",
    "build:example": "tsc --project example && webpack --config example/webpack.conf.js",
    "build:src": "tsc --project src",
    "build:test": "tsc --project test/src",
    "build": "npm run build:src && npm run build:test",
    "test": "mocha test/build/**/*.spec.js",
    "test:coverage": "istanbul cover --dir test/coverage node_modules/mocha/bin/_mocha -- test/build/**/*.spec.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phosphorjs/phosphor.git"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "styles/*.css",
    "styles/**/*.css"
  ],
  "keywords": [
    "algorithm",
    "collections",
    "dom",
    "layout",
    "messaging",
    "properties",
    "signaling",
    "ui",
    "widget"
  ],
  "author": "S. Chris Colbert <sccolbert@gmail.com>",
  "contributors": [
    "A. Darian <git@darian.af>",
    "Dave Willmer <dave.willmer@gmail.com>",
    "S. Chris Colbert <sccolbert@gmail.com>",
    "Steven Silvester <steven.silvester@gmail.com>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phosphorjs/phosphor/issues"
  },
  "homepage": "https://github.com/phosphorjs/phosphor"
}
