{
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "lerna": "2.0.0-beta.38"
  },
  "scripts": {
    "install": "lerna bootstrap --hoist",
    "build": "lerna run build",
    "build:src": "lerna run build --scope \"@phosphor/!(test-)*\"",
    "build:tests": "lerna run build --scope \"@phosphor/test-*\"",
    "clean": "lerna run clean",
    "clean:src": "lerna run clean --scope \"@phosphor/!(test-)*\"",
    "clean:tests": "lerna run clean --scope \"@phosphor/test-*\"",
    "test": "lerna run test --scope \"@phosphor/test-*\""
  }
}
